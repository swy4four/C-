# C_code_store
//013 2022.12.12
/*分段函数求值*/
#include<stdio.h>
int main()
{
	int x, y;
	printf("input a x=");
	scanf_s("%d",&x);
	if (x < 1){
		y = x;
		printf("y =%d\n",y); 
}
	else if (x < 10){
		y = 2 * x - 1;
		printf("y =%d\n",y); 
	}
	else{
		y = 3 * x - 11;
		printf("y=%d\n",y); 
	}
	return 0;
}
